{
  "name": "@effstate/react",
  "version": "0.0.1",
  "description": "React integration for effstate",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@effect-atom/atom-react": "^0.3.0",
    "effect": "^3.0.0",
    "effstate": "workspace:*",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@effect-atom/atom-react": "^0.3.4",
    "@types/react": "^18.3.3",
    "effect": "^3.19.12",
    "effstate": "workspace:*",
    "react": "^18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/handfish/effstate.git",
    "directory": "packages/react"
  }
}
